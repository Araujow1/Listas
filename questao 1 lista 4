#include <stdio.h>
#include <stdlib.h>

int main() {
    int array[6];
    int i;

    printf("Digite seis números inteiros:\n");

    for(i = 0; i < 6; i++) {
        scanf("%d", &array[i]);
    }

    printf("Os números digitados foram:\n");

    for(i = 0; i < 6; i++) {
        printf("%d ", array[i]);
    }

    return 0;
}
